#: projectmanager/templates/about.html:31
msgid "Design my URL structure"
msgstr ""

#: projectmanager/templates/about.html:32
msgid "The \""
msgstr ""

#: projectmanager/templates/about.html:32
msgid "root.py"
msgstr ""

#: projectmanager/templates/about.html:32
msgid "\" file under the"
msgstr ""

#: projectmanager/templates/about.html:33
